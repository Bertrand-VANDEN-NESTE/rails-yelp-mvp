<%= form_with model: @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>
<%= link_to "Back to index", restaurants_path %>
<%= link_to "Back to #{@restaurant.name} show", restaurant_path(@restaurant) %>
